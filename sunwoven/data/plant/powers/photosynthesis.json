{
  "type": "origins:multiple",
  "prevent_food": {
    "type": "origins:prevent_item_use",
    "item_condition": {
      "type": "origins:food"
    }
  },
  "eat": {
    "type":"origins:action_over_time",
    "interval":20,
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:resource",
          "resource": "plant:sunmeter",
          "comparison": ">",
          "compare_to": 0
        },
        {
          "type": "origins:food_level",
          "comparison": "<",
          "compare_to": 20
        }
      ]
    },
    "entity_action":{
      "type": "origins:and",
      "actions": [
        {
          "type":"origins:feed",
          "food":2,
          "saturation":0.5
        },
        {
          "type": "origins:change_resource",
          "resource": "plant:sunmeter",
          "change": -20
        }
      ]
    }
  },
  "disable_regen":{
    "type":"origins:disable_regen"
  },
  "heal_override":{
    "type":"origins:action_over_time",
    "interval":30,
    "entity_action":{
      "type":"origins:heal",
      "amount":1
    },
    "condition":{
      "type":"origins:food_level",
      "comparison":">",
      "compare_to": 18
    }
  },
  "exhaust_when_healing":{
    "type":"origins:action_over_time",
    "interval":30,
    "entity_action":{
      "type":"origins:exhaust",
      "amount":6
    },
    "condition":{
      "type":"origins:and",
      "conditions":[
        {
          "type":"origins:food_level",
          "comparison":">",
          "compare_to": 18
        },
        {
          "type":"origins:relative_health",
          "comparison":"<",
          "compare_to":1
        }
      ]
    }
  }
}
